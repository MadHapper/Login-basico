<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login"

        tools:targetApi="31">
        <activity
            android:name=".listContact.ui.ListContactActivity"
            android:exported="false"
            android:label="@string/title_activity_list_contact"
            android:theme="@style/Theme.Login" />
        <activity
            android:name=".LoadUserActivity"
            android:exported="false"
            android:label="@string/title_activity_load_user"
            android:theme="@style/Theme.Login"></activity>
        <activity
            android:name=".UsuarioActivity"
            android:exported="false"
            android:label="@string/title_activity_usuario"
            android:theme="@style/Theme.Login" />
        <activity
            android:name=".login.ui.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>